{
  "name": "Maartens Search Engine",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Volledig custom zoekmachine",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
 "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "all_frames": true
    },
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["start.js"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "http://*/*",
    "https://*/*"
  ],
  "incognito": "split",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Show me the shit",
    "default_popup": "popup.html"
  }
}